{
    "name": "Leetcode Premium Unlocker",
    "version": "0.3.0",
    "description": "Inject crowdsourced Leetcode premium data",
    "permissions": ["https://sheets.googleapis.com/v4/spreadsheets*"],
    "browser_action": {
      "default_popup": "dist/index.html"
    },
    "manifest_version": 2, 
    "content_scripts": [
        {
          "js": ["/dist/main.js"],
          "matches": [
            "https://leetcode.com/*"
          ]
        }
      ], 
    "icons": {
      "96": "/assets/logo.png"
    }
}
